{
	".clang-format template for C++": {
		"prefix": "clangformat-cpp",
		"body": [
			"BasedOnStyle: Google",
			"IndentWidth: 4",
			"DerivePointerAlignment: false"
		],
		"description": ".clang-format template",
		"isFileTemplate": true
	},
	".clang-format template for C": {
		"prefix": "clangformat-c",
		"body": [
			"BasedOnStyle: Google",
			"IndentWidth: 4",
			"DerivePointerAlignment: false",
            "PointerAlignment: Right"
		],
		"description": ".clang-format template for C",
		"isFileTemplate": true
	},
	".clang-tidy template": {
		"prefix": "clangtidy",
		"body": [
			"FormatStyle: google"
		],
		"description": ".clang-tidy template",
		"isFileTemplate": true
	},
	".clangd suppress builtin_definition": {
		"prefix": "supb",
		"body": [
			"Diagnostics:",
    		"\tSuppress: 'builtin_definition'"
		],
		"description": ".clangd suppress builtin_definition"
	}
}
